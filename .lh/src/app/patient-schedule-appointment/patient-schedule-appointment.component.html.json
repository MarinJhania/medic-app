            "content": "<div class=\"dashboard-container\">\r\n  <app-patient-sidebar-menu></app-patient-sidebar-menu>\r\n\r\n  <div class=\"main-content\">\r\n    <section class=\"schedule-appointment-section\">\r\n      <h1 class=\"section-title\">Agendar Nueva Cita</h1>\r\n      <p class=\"section-subtitle\">Sigue los pasos para encontrar y reservar tu próxima consulta médica.</p>\r\n\r\n      <div class=\"scheduling-steps-container\">\r\n        <div class=\"scheduling-step\">\r\n            <h3>1. Selecciona una Especialidad</h3>\r\n            <div class=\"specialty-list\">\r\n                <button class=\"specialty-item\">Medicina General</button>\r\n                <button class=\"specialty-item\">Cardiología</button>\r\n                <button class=\"specialty-item\">Dermatología</button>\r\n                <button class=\"specialty-item\">Pediatría</button>\r\n                </div>\r\n        </div>\r\n\r\n        <hr class=\"step-divider\"> <div class=\"scheduling-step\">\r\n            <h3>2. Selecciona un Doctor</h3>\r\n            <div class=\"doctor-list\">\r\n                <div class=\"doctor-item\">\r\n                    <span class=\"doctor-name\">Dr. García Martínez</span>\r\n                    <span class=\"doctor-specialty\">(Medicina General)</span>\r\n                    <button class=\"btn btn-select-doctor\">Seleccionar</button>\r\n                </div>\r\n                 <div class=\"doctor-item\">\r\n                    <span class=\"doctor-name\">Dra. Rodríguez López</span>\r\n                    <span class=\"doctor-specialty\">(Medicina General)</span>\r\n                    <button class=\"btn btn-select-doctor\">Seleccionar</button>\r\n                </div>\r\n                </div>\r\n        </div>\r\n\r\n         <hr class=\"step-divider\"> <div class=\"scheduling-step\">\r\n            <h3>3. Selecciona Fecha y Horario</h3>\r\n            <div class=\"date-time-selection\">\r\n                <div class=\"form-group date-picker-group\">\r\n                    <label for=\"schedule-date\">Fecha:</label>\r\n                    <div class=\"input-with-icon\">\r\n                         <input type=\"date\" id=\"schedule-date\">\r\n                         <i class=\"icon fas fa-calendar-alt\"></i> </div>\r\n                </div>\r\n\r\n                <div class=\"form-group time-picker-group\">\r\n                    <label>Horario disponible:</label>\r\n                     <div class=\"time-slots\">\r\n                        <button class=\"time-slot\">09:00</button>\r\n                        <button class=\"time-slot\">09:30</button>\r\n                        <button class=\"time-slot\">10:00</button>\r\n                        <button class=\"time-slot\">10:30</button>\r\n                        </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n      </div>\r\n\r\n\r\n      <div class=\"form-actions\">\r\n          <button class=\"btn btn-cancel\">Cancelar</button>\r\n          <button class=\"btn btn-primary\">Confirmar Cita</button>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</div>"
